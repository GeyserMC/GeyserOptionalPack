{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.rabbit": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.brown",
            "Texture.white",
            "Texture.black",
            "Texture.white_splotched",
            "Texture.gold",
            "Texture.salt"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "q.get_name == 'Toast' ? Texture.toast : (q.is_bribed ? Texture.caerbannog : Array.skins[q.variant])"
      ]
    }
  }
}